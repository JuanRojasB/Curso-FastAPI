{
"dashboard": {
    "title": "Centro Est√©tico - Monitoreo API",
    "panels": [
    {
        "title": "Reservas por minuto",
        "type": "graph",
        "targets": [
        {
            "expr": "rate(spa_reservas_total[5m])",
            "legendFormat": "Reservas"
        }
        ]
    },
    {
        "title": "Tratamientos solicitados",
        "type": "graph",
        "targets": [
        {
            "expr": "rate(spa_tratamientos_solicitados_total[5m])",
            "legendFormat": "Tratamientos"
        }
        ]
    },
    {
        "title": "Tiempo de respuesta API",
        "type": "graph",
        "targets": [
        {
            "expr": "histogram_quantile(0.95, rate(spa_response_duration_seconds_bucket[5m]))",
            "legendFormat": "95th percentile"
        }
        ]
    },
    {
        "title": "Recursos del sistema",
        "type": "graph",
        "targets": [
        {
            "expr": "spa_cpu_usage_percent",
            "legendFormat": "CPU Usage"
        },
        {
            "expr": "spa_memory_usage_bytes / 1024 / 1024",
            "legendFormat": "Memory (MB)"
        }
        ]
    }
    ]
}
}